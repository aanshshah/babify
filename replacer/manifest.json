{
	"manifest_version": 2,
	"name": "Word Replacer",
	"version": "1.0",
	"description": "Replaces a simple word or group of words!",
	"icons": { "16": "images/carbs16.png",
           "48": "images/carbs48.png",
          "128": "images/carbs128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"], 
			"js": ["myscript.js"],
			"run_at": "document_end"  
		}
	]
}